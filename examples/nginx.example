upstream uwsgi_Excel_tech {
        server unix:/opt/Excel_tech/uwsgi/uwsgi.sock;
}

server {
        listen 80;
        server_name exceltech.com.bd www.exceltech.com.bd;

        access_log /opt/Excel_tech/logs/access.log;
        error_log /opt/Excel_tech/logs/error.log;

        charset utf-8;
        client_max_body_size 16M;

        location /static {
                alias /opt/Excel_tech/static;
        }

        location /media {
                alias /opt/Excel_tech/media;
        }

        location / {
                uwsgi_pass uwsgi_Excel_tech;
                include uwsgi_params;
                uwsgi_read_timeout 300s;
                uwsgi_send_timeout 300s;
        }
}